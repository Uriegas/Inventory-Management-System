<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<Scene xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="com.TeamPro.Login.LoginController">
    <AnchorPane prefHeight="473.0" prefWidth="544.0" style="-fx-background-color: #FFFFFF;" styleClass="background-login" stylesheets="@estilos.css">
      <children>
         <ImageView fitHeight="303.0" fitWidth="397.0" layoutX="78.0" layoutY="109.0" pickOnBounds="true" styleClass="shadow">
            <image>
               <Image url="@login_fondito.png" />
            </image>
         </ImageView>
         <Button fx:id="btnLogin" layoutX="162.0" layoutY="350.0" mnemonicParsing="false" onAction="#clickLogin" prefHeight="30.0" prefWidth="224.0" styleClass="button-login" stylesheets="@estilos.css" text="INICIAR SESIÓN" textFill="WHITE">
            <font>
               <Font name="Roboto Bold" size="13.0" />
            </font></Button>
         <ImageView fitHeight="116.0" fitWidth="119.0" layoutX="217.0" layoutY="51.0">
            <image>
               <Image url="@icon.png" />
            </image>
         </ImageView>
         <ImageView fitHeight="61.0" fitWidth="308.0" layoutX="120.0" layoutY="184.0" pickOnBounds="true">
            <image>
               <Image url="@fondo_cuadrito.png" />
            </image>
         </ImageView>
         <ImageView fitHeight="61.0" fitWidth="308.0" layoutX="120.0" layoutY="261.0" pickOnBounds="true">
            <image>
               <Image url="@fondo_cuadrito.png" />
            </image>
         </ImageView>
         <TextField fx:id="TfUsuario" layoutX="176.0" layoutY="197.0" prefHeight="35.0" prefWidth="237.0" promptText="Dirección de correo" style="-fx-background-color: false;" styleClass="jfx-text-field" />
         <TextField fx:id="TfContra" layoutX="176.0" layoutY="278.0" prefHeight="26.0" prefWidth="242.0" promptText="Contrasena" style="-fx-background-color: false;" styleClass="jfx-text-field" />
         <ImageView fitHeight="34.0" fitWidth="33.0" layoutX="129.0" layoutY="198.0" pickOnBounds="true" preserveRatio="true">
            <image>
               <Image url="@user.png" />
            </image>
         </ImageView>
         <ImageView fitHeight="34.0" fitWidth="33.0" layoutX="131.0" layoutY="275.0" pickOnBounds="true" preserveRatio="true">
            <image>
               <Image url="@password.png" />
            </image>
         </ImageView>
      </children></AnchorPane>
</Scene>
